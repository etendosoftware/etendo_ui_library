{
  "name": "etendo-ui-library",
  "version": "1.4.25",
  "private": false,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --updateSnapshot",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "prestart": "yarn update-stories",
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
    "storybook": "start-storybook -p 6006 -c .storybook watch-css -s ./",
    "build-storybook": "build-storybook",
    "build": "yarn build:web && yarn build:native",
    "build:web": "tsc --emitDeclarationOnly --outDir dist-web && babel src --out-dir dist-web --extensions '.ts,.tsx' --config-file ./babel.config.web.js",
    "build:native": "tsc --emitDeclarationOnly --outDir dist-native && babel src --out-dir dist-native --extensions '.ts,.tsx' --config-file ./babel.config.native.js",
    "nextjs": "cd examples/nextjs && yarn install && yarn dev",
    "copy-to-reactnative": "rsync -av --exclude='__tests__' --exclude='__snapshots__' --exclude='*.test.*' src/ ./examples/ReactNativeTS706",
    "copy-to-nextjs": "rsync -av --exclude='__tests__' --exclude='__snapshots__' --exclude='*.test.*' src/ ./examples/nextjs/apps/web/app",
    "copy-to-storybook": "rsync -av --exclude='__tests__' --exclude='__snapshots__' --exclude='*.test.*' src/ ./examples/storybook",
    "watch": "nodemon --watch src -e js,jsx,ts,tsx --exec \"concurrently 'yarn copy-to-reactnative' 'yarn copy-to-nextjs' 'yarn copy-to-storybook' \""
  },
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "files": [
    "dist-web/**/*.js",
    "dist-web/**/*.d.ts",
    "dist-native/**/*.js",
    "dist-native/**/*.d.ts",
    "index.d.ts"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-svg": "*",
    "react-native-svg-web": "*",
    "react-native-web": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.20.0",
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-clipboard/clipboard": "^1.11.1",
    "@react-native-community/datetimepicker": "^6.1.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native-community/slider": "^4.4.2",
    "@testing-library/react-native": "^11.1.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.9",
    "@types/react": "^18.2.38",
    "@types/react-native": "^0.72.2",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/react-test-renderer": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^29.2.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-native": "0.70.6",
    "react-native-document-picker": "^9.1.0",
    "react-native-svg": "^13.9.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "^0.19.4",
    "react-syntax-highlighter": "^15.5.0",
    "react-test-renderer": "18.2.0",
    "rehype-highlight": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "typescript": "^5.3.2"
  },
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "examples/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.7",
    "@mui/styles": "^5.15.8"
  }
}
